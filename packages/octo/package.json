{
  "dependencies": {},
  "description": "Octo is an opinionated interface of a cloud infrastructure.",
  "devDependencies": {},
  "engines": {
    "node": ">=16"
  },
  "exports": "./dist/index.js",
  "files": [
    "dist",
    "!dist/tsconfig.build.tsbuildinfo",
    "package.json"
  ],
  "jest": {
    "coverageDirectory": "./coverage",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "preset": "ts-jest/presets/default-esm",
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".*spec\\.ts$",
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "main": "./dist/index.js",
  "name": "@quadnix/octo",
  "nx": {
    "tags": [
      "scope:octo"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "check:circular-dependency": "dpdm --no-tree --exit-code circular:1 --no-warning ./dist/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --testRegex=\"\\.spec.ts$\"",
    "test:cov": "jest --coverage --testRegex=\"\\.spec.ts$\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --testRegex=\"\\.e2e-spec.ts$\"",
    "test:integration": "jest --testRegex=\"\\.ispec.ts$\"",
    "test:watch": "jest --watch"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.0.22"
}
