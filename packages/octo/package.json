{
  "dependencies": {},
  "description": "Octo is an opinionated interface of a cloud infrastructure.",
  "devDependencies": {},
  "jest": {
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "./test/jest.setup.ts"
    ],
    "testEnvironment": "node",
    "testRegex": ".*spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "main": "dist/index.js",
  "name": "@quadnix/octo",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --testRegex=\"\\.spec.ts$\"",
    "test:cov": "jest --coverage --testRegex=\"\\.spec.ts$\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --testRegex=\"\\.e2e-spec.ts$\"",
    "test:integration": "jest --testRegex=\"\\.ispec.ts$\"",
    "test:watch": "jest --watch"
  },
  "version": "0.0.1"
}
