{
  "dependencies": {
    "@aws-sdk/client-ecr": "~3.370.0",
    "@aws-sdk/client-ecs": "~3.370.0",
    "@aws-sdk/client-s3": "~3.374.0",
    "@aws-sdk/lib-storage": "~3.379.1",
    "@quadnix/octo": "~0.0.1",
    "mime": "~3.0.0"
  },
  "description": "Octo-AWS-CDK is an AWS specific implementation of the Octo interface.",
  "devDependencies": {},
  "jest": {
    "coverageDirectory": "./coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "setupFilesAfterEnv": [
      "./test/jest.setup.ts"
    ],
    "testEnvironment": "node",
    "testRegex": ".*spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "main": "dist/index.js",
  "name": "@quadnix/octo-aws-cdk",
  "nx": {
    "tags": [
      "scope:octo-aws-cdk"
    ]
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --testRegex=\"\\.spec.ts$\"",
    "test:cov": "jest --coverage --testRegex=\"\\.spec.ts$\"",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --testRegex=\"\\.e2e-spec.ts$\"",
    "test:integration": "jest --testRegex=\"\\.ispec.ts$\"",
    "test:watch": "jest --watch"
  },
  "version": "0.0.1"
}
