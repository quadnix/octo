{
  "dependencies": {},
  "description": "<%= description %>",
  "devDependencies": {},
  "engines": {
    "node": ">=20.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "preset": "ts-jest/presets/default-esm",
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".*spec\\.ts$",
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "keywords": [
    "cdk",
    "cicd",
    "infrastructure",
    "octo"
  ],
  "main": "./dist/index.js",
  "name": "<%= name %>",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "check:circular-dependency": "dpdm --no-tree --exit-code circular:1 --no-warning ./dist/**/*.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --testRegex=\"\\.spec.ts$\"",
    "test:cov": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --testRegex=\"\\.spec.ts$\"",
    "test:e2e": "NODE_OPTIONS=--experimental-vm-modules jest --testRegex=\"\\.e2e-spec.ts$\"",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --testRegex=\"\\.ispec.ts$\""
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "<%= version %>"
}
